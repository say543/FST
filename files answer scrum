    | join UxoConnection on CorrelationId | where MicrosoftEyesOnCortanaLogs == 1
    | project CorrelationId
    | take 1
        | join DebugPersonalInfo on $left.CorrelationId == $right.EventTraceId
        | project EventTraceId, TIMESTAMP, UserId, TenantId, DocId, DocType, TotalPages, PageId, Payload
        | order by DocType asc, PageId asc
        | summarize  make_list(Payload) by DocType
        | extend grammar = strcat_array(list_Payload,'')
        | project DocType, grammar

